<style>
       #map {
        height: 350px;
        width: 350px;
       }
    </style>
<div id="map"></div>
<script>
  function initMap() {
    var uluru = {lat: <%=@foodspot.latitude%>, lng: <%=@foodspot.longitude%>};
    var map = new google.maps.Map(document.getElementById('map'), {
      zoom: 15,
      center: uluru
    });
    var marker = new google.maps.Marker({
      position: uluru,
      map: map
    });
  }
</script>
<script async defer
src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAkS0GoUpPEJ_X8JXxTJmMOpB02BTn3-nU&callback=initMap">
</script>
